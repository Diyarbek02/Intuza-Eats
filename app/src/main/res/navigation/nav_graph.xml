<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.diyarbekDev.intuzaeats.ui.Fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_menuFragment"
            app:destination="@id/menuFragment"
            app:popUpTo="@id/menuFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/menuFragment"
        tools:layout="@layout/fragment_menu"
        android:name="com.diyarbekDev.intuzaeats.ui.Fragments.MenuFragment"
        android:label="fragment_menu" />
    <fragment
        android:id="@+id/shoppingCartFragment"
        tools:layout="@layout/fragment_shopping_cart"
        android:name="com.diyarbekDev.intuzaeats.ui.Fragments.ShoppingCartFragment"
        android:label="fragment_shopping_cart" />
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.diyarbekDev.intuzaeats.ui.Fragments.AboutUsFragment"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.diyarbekDev.intuzaeats.ui.Fragments.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />

</navigation>